{
    "name": "express-mysql-setup",
    "version": "1.0.0",
    "type": "module",
    "description": "Express.js + MySQL example with connection pooling",
    "main": "src/index.js",
    "scripts": {
        "start": "node src/index.js",
        "dev": "nodemon src/index.js",
        "migrate": "knex --knexfile knexfile.js migrate:latest",
