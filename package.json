        "express-rate-limit": "^8.1.0",
        "helmet": "^8.1.0",
        "jsonwebtoken": "^9.0.0",
        "mysql2": "^3.2.0"
    },
    "devDependencies": {
        "knex": "^3.1.0",
        "nodemon": "^3.1.10"
    }
}
